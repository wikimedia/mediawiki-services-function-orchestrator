{
	"root": true,
	"extends": [
		"wikimedia/server"
	],
	"rules": {
		"camelcase": "off",
		"no-use-before-define": "off",
		"no-shadow": "off",
		"jsdoc/no-undefined-types": "off",
		"no-underscore-dangle": "off"
	},
	"overrides": [
		{
			"files": "**/*.json",
			"extends": "wikimedia/json"
		}
	]
}
